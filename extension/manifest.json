{
  "name": "Tweet View",
  "version": "1.0",
  "description": "Hide negative tweets in your feed.",
  "icons": {"19": "active.png"},
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-3.0.0.min.js","bundle.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Tweet View",
    "default_icon": "active.png"
  },
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2
}